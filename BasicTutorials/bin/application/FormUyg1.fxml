<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="200.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/15.0.1" fx:controller="application.FormUyg1Controller">
   <children>
      <VBox prefHeight="400.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <ImageView fitHeight="123.0" fitWidth="200.0" nodeOrientation="RIGHT_TO_LEFT" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@a.png" />
                     </image>
                  </ImageView>
               </children>
            </HBox>
            <Button fx:id="btnAnaMenü" mnemonicParsing="false" onAction="#btnAnaMenü_click" prefHeight="36.0" prefWidth="200.0" text="AnaMenü" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Button>
            <Button fx:id="btnİletisim" mnemonicParsing="false" onAction="#btnİletisim_click" prefHeight="36.0" prefWidth="200.0" text="İletişim" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Button>
            <Button fx:id="btnUyeler" mnemonicParsing="false" onAction="#btnUyeler_click" prefHeight="36.0" prefWidth="200.0" text="Üyeler" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Button>
            <Button fx:id="btnAyarlar" mnemonicParsing="false" onAction="#btnAyarlar_click" prefHeight="36.0" prefWidth="200.0" text="Ayarlar" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Button>
            <Button fx:id="btnCikis" mnemonicParsing="false" onAction="#btnCikis_click" prefHeight="36.0" prefWidth="200.0" text="Çıkış" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
